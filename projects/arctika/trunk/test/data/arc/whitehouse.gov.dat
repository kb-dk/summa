<script type="text/javascript" language="javascript">
<!--
      document.write(lt+"a href=\"#\" onClick=\"javascript:popup('/news/releases/2006/09/20060928.v.html','394','316')\" "+gt+""+lt+"b"+gt+""+lt+"img src="+dquot+"/news/releases/2006/09/images/20060928_vidbtn68h.jpg"+dquot+" name="+dquot+"vidbtn"+dquot+" class="+dquot+"homepage-thumb"+dquot+" alt="+dquot+"Play Video"+dquot+" border="+dquot+"0"+dquot+" align="+dquot+"left"+dquot+"/"+gt+""+lt+"/b"+gt+""+lt+"/a"+gt+"");
